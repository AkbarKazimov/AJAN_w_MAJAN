	ajan:event agents:PopulateLAREvent ;
	bt:content [
					a bt:ConstructQuery ;
					bt:originBase ajan:AgentKnowledge;
					bt:sparql """
						PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> 
