:SolvingCHCBT
	a bt:BehaviorTree ;
	a bt:Root ;
	rdfs:label "SolvingCHCBT Behavior Tree" ;
	bt:hasChild [
					a bt:Priority ;
					bt:hasChildren (
							[
								a bt:Sequence ;
								bt:hasChildren (
										:HandleSolveCHCEvent
										:UpdateRemoveDuplicateSimilarities
										:WriteComputedReciprocalSimilarities
										:HDBSCAN
										:BroadcastHDBSCANResult
